  "scripts": {
    "cashier": "nx run cashier:serve --port=4201 -o",
    "store": "nx run store:serve --port=4202 -o",
    "admin": "nx run admin:serve --port=4200 -o",
    "cashier-e2e": "nx e2e cashier-e2e --ui",
    "store-e2e": "nx e2e store-e2e --ui",
    "admin-e2e": "nx e2e admin-e2e --ui",
    "cashier-prod": "nx run cashier:build --configuration=production",
    "store-prod": "nx run store:build --configuration=production",
    "admin-prod": "nx run admin:build --configuration=production",
    "cashier-dev": "nx run cashier:build --configuration=development",
    "store-dev": "nx run store:build --configuration=development",
    "admin-dev": "nx run admin:build --configuration=development",
    "deploy-admin": "nx run admin:build --prod --output-path=./dist/admin --skip-nx-cache",
    "deploy": "gh-pages -d dist/admin/browser"
  },
  "private": true,
